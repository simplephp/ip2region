{
    "name": "simplephp/ip2region",
    "type": "library",
    "license": "MIT",
    "version": "1.1.8",
    "authors": [
        {
            "name": "simplephp",
            "email": "onlineservice120@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "ip2region"
    ],
    "description": "ip2region for hyperf",
    "autoload": {
        "psr-4": {
            "Simplephp\\Ip2region\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Simplephp\\Ip2region\\ConfigProvider"
        }
    }
}
